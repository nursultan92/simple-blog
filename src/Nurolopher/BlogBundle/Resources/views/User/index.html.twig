{% extends "::base.html.twig" %}
{% block body %}
    <a href="{{ path('nurolopher_blog_user_new') }}">Create new</a>
    <ul>
    {% for user in users %}
        <li>
            <a href="{{ path('nurolopher_blog_user_show',{'id': user.id}) }}">{{ user.email }}</a>
            {{ user.firstname }}
            {{ user.lastname }}
            <a href="{{ path('nurolopher_blog_user_edit',{'id':user.id}) }}">Edit</a>
            <a href="{{ path('nurolopher_blog_user_delete',{id:user.id}) }}">Delete</a>
        </li>
    {% endfor %}
    </ul>
{% endblock %}